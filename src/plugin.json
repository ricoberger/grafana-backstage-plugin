{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Backstage",
  "id": "ricoberger-backstage-app",
  "backend": true,
  "executable": "gpx_backstage",
  "info": {
    "keywords": [
      "app",
      "backstage"
    ],
    "description": "Backstage plugin for Grafana. Explore your service catalog.",
    "author": {
      "name": "Rico Berger"
    },
    "links": [
      {
        "name": "Repository",
        "url": "https://github.com/ricoberger/grafana-backstage-plugin"
      },
      {
        "name": "Documentation",
        "url": "https://github.com/ricoberger/grafana-backstage-plugin/blob/main/README.md"
      },
      {
        "name": "Issues",
        "url": "https://github.com/ricoberger/grafana-backstage-plugin/issues"
      },
      {
        "name": "License",
        "url": "https://github.com/ricoberger/grafana-backstage-plugin/blob/main/LICENSE"
      },
      {
        "name": "Sponsorship",
        "url": "https://github.com/ricoberger"
      }
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Catalog",
        "path": "img/screenshots/catalog.png"
      },
      {
        "name": "Dashboard: Backstagee Entity",
        "path": "img/screenshots/dashboard-backstage-entity.png"
      },
      {
        "name": "Dashboard: Backstage Entity (Groups and Users)",
        "path": "img/screenshots/dashboard-backstage-entity-groups-and-users.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "panel",
      "name": "Backstage: Catalog Entities",
      "path": "panel-catalog-entities/plugin.json"
    },
    {
      "type": "panel",
      "name": "Backstage: Catalog Entity",
      "path": "panel-catalog-entity/plugin.json"
    },
    {
      "type": "panel",
      "name": "Backstage: Catalog Graph",
      "path": "panel-catalog-graph/plugin.json"
    },
    {
      "type": "page",
      "name": "Catalog",
      "path": "/a/%PLUGIN_ID%/catalog",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "icon": "cog",
      "name": "Configuration",
      "path": "/plugins/%PLUGIN_ID%",
      "role": "Admin",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=12.0.0",
    "plugins": []
  }
}
